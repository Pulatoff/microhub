{
    "extends": ["prettier"],
    "rules": {
        "no-console": "warn",
        "no-debugger": "error",
        "no-unused-vars": "warn",
        "no-const-assign": "error"
    },
    "plugins": ["prettier", "eslint-plugin-prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "env": {
        "commonjs": true,
        "node": true,
        "browser": true
    }
}
